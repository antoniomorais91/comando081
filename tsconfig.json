{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/components/Head.jsx", "src/components/Maps.jsx", "src/components/How.jsx", "src/components/Header.jsx", "src/components/Footer.jsx", "src/components/Command.jsx", "src/components/Navbar.jsx", "src/components/Logo.jsx", "src/components/Icons.jsx", "src/components/Units.jsx", "src/components/Swiper2.jsx", "src/components/Swiper.tsx"],
  "exclude": ["node_modules"]
}
